{
    "name": "edrands/laravel-seeder",
    "description": "The Enhanced Laravel Project Seeder gets new Laravel projects up-and-running fast. Features self-contained testing (no pear or global testing package required).",
    "keywords": ["laravel", "seeder", "starter", "project"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=5.4.0",
        "patricktalmadge/bootstrapper": "~4.1.14",
        "laravel/framework": "4.2.*@stable"
    },
    "require-dev": {
        "way/laravel-test-helpers": "dev-master",
        "mockery/mockery": "dev-master@dev",
        "davedevelopment/hamcrest-php": "dev-master",
        "barryvdh/laravel-debugbar": "1.*@stable",
        "barryvdh/laravel-ide-helper": "1.*@stable"
    },
    "autoload": {
        "classmap": [
            "app/commands",
            "app/controllers",
            "app/models",
            "app/database/migrations",
            "app/database/seeds",
            "app/tests/TestCase.php"
        ]
    },
    "scripts": {
        "post-install-cmd": [
            "php artisan clear-compiled",
            "php artisan ide-helper:generate",
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "php artisan clear-compiled",
            "php artisan ide-helper:generate",
            "php artisan debugbar:publish",
            "php artisan optimize"
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ]
    },
    "config": {
        "preferred-install": "dist"
    },
    "minimum-stability": "stable"
}
